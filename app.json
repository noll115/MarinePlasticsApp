{
  "expo": {
    "name": "Marine Plastics",
    "slug": "MarinePlasticsApp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "scheme": "marineplastics",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/square-coi.png",
    "splash": {
      "image": "./assets/images/square-coi.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.marineplastics.marineplasticsapp"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist" : {
          "NSCameraUsageDescription" : "Allow Marine Plastics to Access Your Camera"
      }
    }

  }
}
