{
  "expo": {
    "name": "Marine Plastics",
    "slug": "MarinePlasticsApp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/android-chrome-256x256.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist" : {
          "NSCameraUsageDescription" : "Allow Marine Plastics to Access Your Camera"
      }
    }

  }
}
